# platform :ios, '9.0'
# Podfile
use_frameworks!

target 'TestApp' do
    pod 'RxSwift', '6.2.0'
    pod 'RxCocoa', '6.2.0'
end

# RxTest and RxBlocking make the most sense in the context of unit/integration tests
target 'TestAppTests' do
    pod 'RxBlocking', '6.2.0'
    pod 'RxTest', '6.2.0'
end

# RxTest and RxBlocking make the most sense in the context of unit/integration tests
target 'TestAppUITests' do
    pod 'RxBlocking', '6.2.0'
    pod 'RxTest', '6.2.0'
end